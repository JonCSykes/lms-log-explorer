{
  "name": "lms-log-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "pnpm test:unit",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.config.ts lib/parser/__tests__ lib/indexer/__tests__",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:install": "playwright install chromium",
    "db:reset": "node scripts/reset-index-db.mjs",
    "db:reset:docker": "docker compose down --volumes --remove-orphans",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --project tsconfig.json --pretty false"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@genkit-ai/anthropic": "^0.2.0",
    "@genkit-ai/compat-oai": "^1.28.0",
    "@genkit-ai/google-genai": "^1.28.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "genkit": "^1.28.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.5",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.7.0",
    "shadcn": "^3.8.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.35",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vercel/style-guide": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "postcss": "^8.4.47",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
